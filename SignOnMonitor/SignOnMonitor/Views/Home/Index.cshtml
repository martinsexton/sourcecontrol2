@model List<SignOnMonitor.Models.SignOnRequest>

<div class="panel panel-default">
    <div class="panel-heading"><h3>Sign-on Requests</h3></div>
    <div class="scrolling panel-body col-md-12">
        @{
            foreach (var item in ViewData.Model)
            {
                <div class="row">
                    <div class="col-md-9">
                        <h4>@item.Username, Photo Match: @item.Success, Location: (@item.Longitude, @item.Latitude)</h4>
                    </div>
                    <div class="col-md-3">
                        @Html.ActionLink("View Map", "ViewMap", new { id = @item.Id })
                    </div>
                </div>
                <!--<div class="row col-md-6">
                    <h4>@item.Username, Photo Match: @item.Success, Longitude: @item.Longitude, Latitude: @item.Latitude</h4>@Html.ActionLink("View Map", "ViewMap", new {  id = @item.Id})
                </div>-->
            }
}
    </div>
</div>
